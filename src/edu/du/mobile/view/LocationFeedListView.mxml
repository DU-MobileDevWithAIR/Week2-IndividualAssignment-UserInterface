<s:View xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns:s="library://ns.adobe.com/flex/spark">

    <s:Label text="Done" color="0x3380DD" textDecoration="underline" width="100%" textAlign="right" paddingRight="20" paddingTop="10" />

        <s:Panel title="Nearby Locations"
                 width="90%" height="80%"
                 horizontalCenter="0" verticalCenter="10">
            <s:VGroup left="10" right="20" top="20" bottom="20">
                <s:List id="list">
                    <s:itemRenderer>
                        <fx:Component>
                            <s:ItemRenderer>
                                <s:Label id="numLabel" text="Item {data.itemNum}" top="5" left="5" right="5" />
                            </s:ItemRenderer>
                        </fx:Component>
                    </s:itemRenderer>
                    <s:dataProvider>
                        <s:ArrayList>
                            <fx:Object itemNum="One" />
                            <fx:Object itemNum="Two" />
                            <fx:Object itemNum="Three" />
                        </s:ArrayList>
                    </s:dataProvider>
                </s:List>
            </s:VGroup>
        </s:Panel>

</s:View>