<?xml version="1.0" encoding="UTF-8"?>
<project name="module_week2-individualassignment-userinterface" default="compile.module.week2-individualassignment-userinterface">
  <dirname property="module.week2-individualassignment-userinterface.basedir" file="${ant.file.module_week2-individualassignment-userinterface}"/>
  
  <property name="module.jdk.home.week2-individualassignment-userinterface" value="${jdk.home.apache_sdk}"/>
  <property name="module.jdk.bin.week2-individualassignment-userinterface" value="${jdk.bin.apache_sdk}"/>
  <property name="module.jdk.classpath.week2-individualassignment-userinterface" value="jdk.classpath.apache_sdk"/>
  
  <property name="compiler.args.week2-individualassignment-userinterface" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="week2-individualassignment-userinterface.output.dir" value="${module.week2-individualassignment-userinterface.basedir}/out/production/Week2-IndividualAssignment-UserInterface"/>
  <property name="week2-individualassignment-userinterface.testoutput.dir" value="${module.week2-individualassignment-userinterface.basedir}/out/test/Week2-IndividualAssignment-UserInterface"/>
  
  <path id="week2-individualassignment-userinterface.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="week2-individualassignment-userinterface.module.production.classpath">
    <path refid="${module.jdk.classpath.week2-individualassignment-userinterface}"/>
  </path>
  
  <path id="week2-individualassignment-userinterface.runtime.production.module.classpath">
    <pathelement location="${week2-individualassignment-userinterface.output.dir}"/>
  </path>
  
  <path id="week2-individualassignment-userinterface.module.classpath">
    <path refid="${module.jdk.classpath.week2-individualassignment-userinterface}"/>
    <pathelement location="${week2-individualassignment-userinterface.output.dir}"/>
  </path>
  
  <path id="week2-individualassignment-userinterface.runtime.module.classpath">
    <pathelement location="${week2-individualassignment-userinterface.testoutput.dir}"/>
    <pathelement location="${week2-individualassignment-userinterface.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.week2-individualassignment-userinterface">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.week2-individualassignment-userinterface">
    <patternset refid="excluded.from.module.week2-individualassignment-userinterface"/>
  </patternset>
  
  <path id="week2-individualassignment-userinterface.module.sourcepath">
    <dirset dir="${module.week2-individualassignment-userinterface.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.week2-individualassignment-userinterface" depends="compile.module.week2-individualassignment-userinterface.production,compile.module.week2-individualassignment-userinterface.tests" description="Compile module Week2-IndividualAssignment-UserInterface"/>
  
  <target name="compile.module.week2-individualassignment-userinterface.production" depends="register.custom.compilers" description="Compile module Week2-IndividualAssignment-UserInterface; production classes">
    <mkdir dir="${week2-individualassignment-userinterface.output.dir}"/>
    <javac2 destdir="${week2-individualassignment-userinterface.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.week2-individualassignment-userinterface}/javac">
      <compilerarg line="${compiler.args.week2-individualassignment-userinterface}"/>
      <bootclasspath refid="week2-individualassignment-userinterface.module.bootclasspath"/>
      <classpath refid="week2-individualassignment-userinterface.module.production.classpath"/>
      <src refid="week2-individualassignment-userinterface.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.week2-individualassignment-userinterface"/>
    </javac2>
    
    <copy todir="${week2-individualassignment-userinterface.output.dir}">
      <fileset dir="${module.week2-individualassignment-userinterface.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.week2-individualassignment-userinterface.tests" depends="register.custom.compilers,compile.module.week2-individualassignment-userinterface.production" description="compile module Week2-IndividualAssignment-UserInterface; test classes" unless="skip.tests"/>
  
  <target name="clean.module.week2-individualassignment-userinterface" description="cleanup module">
    <delete dir="${week2-individualassignment-userinterface.output.dir}"/>
    <delete dir="${week2-individualassignment-userinterface.testoutput.dir}"/>
  </target>
</project>